        <dc:title>A Little Love Story</dc:title>
        <dc:creator opf:file-as="Merullo, Roland" opf:role="aut">Roland Merullo</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (0.7.35) [http://calibre-ebook.com]</dc:contributor>
