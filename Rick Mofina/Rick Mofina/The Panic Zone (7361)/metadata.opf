        <dc:title>The Panic Zone</dc:title>
        <dc:creator opf:file-as="Mofina, Rick" opf:role="aut">Rick Mofina</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (0.7.35) [http://calibre-ebook.com]</dc:contributor>
